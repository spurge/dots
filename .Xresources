! xft

xft.dpi:                96
xft.antialias:          true
xft.rgba:               rgb
xft.hinting:            true
xft.hintstyle:          hintmedium
xft.lcdfilter:          lcddefault

! urxvt

urxvt.loginShell:       true
urxvt.saveLines:        500
urxvt.internalBorder:   2
urxvt.externalBorder:   0
urxvt.lineSpace:        0
urxvt.scrollStyle:      rxvt
urxvt.scrollBar:        false
urxvt.cursorBlink:      true
urxvt.cursorUnderline:  true

! perl extensions

urxvt.perl-xt-common:  default,clipboard,matcher,keyboard-select
urxvt.keysum.M-u:       perl:url-select:select_next
urxvt.urlLauncher:      /usr/bin/firefox
urxvt.underlineURLs:    true
urxvt.keysym.M-Escape:  perl.keyboard-select:active
urxvt.keysym.M-c:       perl:clipboard:copy
urxvt.keysym.M-v:       perl:clipboard:paste
urxvt.keysym.M-C-v:     perl:clipboard:paste_escaped

! general

*depth:                 32
*allow_bold:            true
*font:                  xft:Fantasque Sans Mono:style=Regular:size=11:antialias=true:hinting=true
*boldFont:              xft:Fantasque Sans Mono:style=Bold:size=11:antialias=true:hinting=true

